# Currently the fastest
CXX=g++-12 CXXFLAGS="-march=native" cmake -GNinja -B build.g++-12 && cmake --build build.g++-12
CXX=g++-10 CXXFLAGS="-march=native" cmake -GNinja -B build.g++-10 && cmake --build build.g++-10
CXX=clang++-14 CXXFLAGS="-march=native" cmake -GNinja  -B build.clang++-14 && cmake --build build.clang++-14

